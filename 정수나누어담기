package 정수나누어담기;

public class ExampleTest2 {
  public static void main(String[] args) {

      Scanner sc = new Scanner(System.in);
      System.out.print("숫자를 입력 하세요 : ");
      int num = sc.nextInt();
      int a = num / 100;
      int b = (num % 100) / 10;
      int c = num % 100;
      System.out.println("100의 자리는  : " + a + " 10의 자리는 : " + b + " 1의 자리는 : " + c); 
  }
}
